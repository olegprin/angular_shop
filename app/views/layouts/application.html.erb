      content='<%#= yield_meta_tag(:description, 'Blog about rails') %>' />
		<meta name='keywords'
      content='<%#= yield_meta_tag(:keywords, 'defaults,ruby,rails, blog') %>' />
  </head>


	<%= yield%>
</html>